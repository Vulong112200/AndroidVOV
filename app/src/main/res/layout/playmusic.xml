<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/reset_passw"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/backscreen"
    >

    <ImageView
        android:id="@+id/back"
        android:layout_width="22dp"
        android:layout_height="18.8dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="365dp"
        android:layout_marginBottom="674dp"
        android:contentDescription="vector"
        android:src="@drawable/vector" />

    <TextView
        android:id="@+id/playnow"
        android:layout_width="195dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="113dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="44dp"
        android:fontFamily="Rubik"
        android:text="Playing Now"
        android:textColor="@color/white"
        android:textSize="28sp"
        android:textStyle="bold" />

    <RelativeLayout
        android:id="@+id/mask_group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="115dp">

        <ImageView
            android:id="@+id/img_song"
            android:layout_width="261dp"
            android:layout_height="261dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:src="@drawable/img_song1" />

        <TextView
            android:id="@+id/__p__mu_n_r_i_m__sao_c_n"
            android:layout_width="260dp"
            android:layout_height="45dp"
            android:layout_alignParentTop="true"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginLeft="5.5dp"
            android:layout_marginTop="278dp"
            android:fontFamily="Arial"
            android:gravity="center"
            android:text="Muộn Rồi Mà Sao Còn"
            android:textColor="@color/title"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/__p__s_n_t_ng_mtp"
            android:layout_width="137dp"
            android:layout_height="29dp"
            android:layout_alignParentTop="true"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginLeft="66dp"
            android:layout_marginTop="311dp"
            android:fontFamily="Arial"
            android:gravity="center"
            android:text="Sơn Tùng MTP"
            android:textColor="@color/sub_title"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/emptyheart"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="291dp"
            android:layout_marginEnd="30dp"
            android:src="@drawable/img_emptyheart" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/group_165"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="494.3dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="205dp">

        <ImageView
            android:id="@+id/volume_"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="320dp"
            android:src="@drawable/img_volume_" />

        <ImageView
            android:id="@+id/repeat"
            android:layout_width="22dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="38dp"
            android:src="@drawable/img_repeat" />

        <ImageView
            android:id="@+id/shuffle"
            android:layout_width="22dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="-1dp"
            android:layout_marginEnd="2dp"
            android:src="@drawable/img_shuffle" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/group_166"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="119dp">

        <TextView
            android:id="@+id/__p___00_00"
            android:layout_width="48dp"
            android:layout_height="22dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="31dp"
            android:layout_marginLeft="31dp"
            android:fontFamily="Arial"
            android:text="00:00"
            android:textColor="@color/sub_title"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/__p___04_00"
            android:layout_width="42dp"
            android:layout_height="22dp"
            android:layout_alignParentEnd="true"
            android:layout_gravity="center_vertical|end"
            android:layout_marginLeft="319dp"
            android:layout_marginEnd="37dp"
            android:fontFamily="Arial"
            android:gravity="end"
            android:text="04:00"
            android:textColor="@color/sub_title"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/processbar"
            android:layout_width="353dp"
            android:layout_height="5.5dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="25dp"
            android:src="@drawable/img_processbar" />

        <ImageView
            android:id="@+id/ellipse"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentEnd="true"

            android:layout_alignParentBottom="true"
            android:layout_marginEnd="366dp"
            android:layout_marginBottom="20dp"
            android:src="@drawable/img_ellipse" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/group_145"
        android:layout_width="175.6dp"
        android:layout_height="40.75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="118dp"
        android:layout_marginTop="702dp"
        android:layout_marginBottom="71dp">

        <ImageView
            android:id="@+id/previous"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="146dp"
            android:src="@drawable/img_previous" />

        <ImageView
            android:id="@+id/pause"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="76dp"
            android:src="@drawable/img_pause" />

        <ImageView
            android:id="@+id/next"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="2dp"
            android:src="@drawable/img_next" />
    </RelativeLayout>
</RelativeLayout>