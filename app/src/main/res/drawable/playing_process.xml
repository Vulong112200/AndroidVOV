<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:layout_width="10dp"
            android:layout_height="1dp"
            android:shape="line">

            <!-- Set màu nền-->
            <solid android:color="#C4C4C4"></solid>

            <!-- đặt thuộc tính màu và độ rộng của đường viền -->
            <stroke
                android:width="0.1dp"
                android:color="#fff"></stroke>
            <corners>

            </corners>

        </shape>

    </item>

    <item android:drawable="@drawable/ellipse"
        android:width="1dp"
        android:height="1dp"
        android:top="10dp"
        android:right="10dp"
        android:left="30dp"
    android:bottom="10dp"
        >

    </item>


</layer-list>